table Intelligence
	lineageTag: 4ee392c1-e225-4de3-9c83-eef95d138903

	calculationGroup

		calculationItem MTD =
				CALCULATE (
				    SELECTEDMEASURE (),
				    DATESMTD ( 'factTaxiDataShort'[date] )
				)

		calculationItem YTD =
				CALCULATE (
				    SELECTEDMEASURE (),
				    DATESYTD ( 'factTaxiDataShort'[date] )
				)

		calculationItem PY =
				CALCULATE (
				    SELECTEDMEASURE (),
				    SAMEPERIODLASTYEAR ( 'date'[Date] )
				)

		calculationItem YOY =
				VAR CY =
				    SELECTEDMEASURE ()
				VAR PY =
				    CALCULATE (
				        SELECTEDMEASURE (),
				        SAMEPERIODLASTYEAR ( 'date'[Date]  )
				    )
				RETURN
				    CY - PY

		calculationItem YOY% =
				VAR CY =
				    SELECTEDMEASURE ()
				VAR PY =
				    CALCULATE (
				        SELECTEDMEASURE (),
				        SAMEPERIODLASTYEAR ( 'date'[Date]  )
				    )
				VAR YOY = CY - PY
				RETURN
				    DIVIDE (
				        YOY,
				        PY,
				        0
				    )

			formatStringDefinition = "0.00%"

		calculationItem Current = SELECTEDMEASURE ()

